/* VS Code Dark+ Theme - Authentic Dark Theme */
/* Based on official VS Code Dark+ color palette */

/* === THEME COLOR VARIABLES === */
/* 
Primary Colors:
- Background: #1e1e1e (editor.background)
- Secondary Background: #252526 (sideBar.background)  
- Surface: #2d2d30 (tab.inactiveBackground)
- Text Primary: #cccccc (foreground)
- Text Secondary: #969696 (tab.inactiveForeground)
- Border: #464647 (panel.border)
- Accent: #007acc (focusBorder, button.background)
- Hover: #2a2d2e (list.hoverBackground)
- Selection: #094771 (list.activeSelectionBackground)
*/

/* === BASE RESET AND TYPOGRAPHY === */
QWidget {
    font-family: "Segoe UI", "Ubuntu", "Droid Sans", sans-serif;
    font-size: 13px;
    background-color: #1e1e1e;
    color: #cccccc;
}

QMainWindow {
    background-color: #1e1e1e;
    color: #cccccc;
    border: none;
    outline: none;
}

/* === PANELS === */
QWidget[objectName="explorer_panel"] {
    background-color: #252526;
    border-right: 1px solid #464647;
}

QWidget[objectName="search_panel"] {
    background-color: #252526;
    border-right: 1px solid #464647;
}

QWidget[objectName="preferences_panel"] {
    background-color: #252526;
    border-right: 1px solid #464647;
}

QWidget[objectName="extensions_panel"] {
    background-color: #252526;
    border-right: 1px solid #464647;
}

QWidget[objectName="account_panel"] {
    background-color: #252526;
    border-right: 1px solid #464647;
}

/* === PANEL TITLES === */
QLabel[objectName="panel_title"] {
    background-color: transparent;
    color: #969696;
    font-weight: normal;
    font-size: 11px;
    padding: 8px 12px 4px 12px;
    text-transform: uppercase;
    letter-spacing: 0.5px;
}

/* === BUTTONS === */
QPushButton {
    background-color: #0e639c;
    border: 1px solid #0e639c;
    color: #ffffff;
    padding: 6px 12px;
    border-radius: 2px;
    font-size: 13px;
    min-height: 22px;
}

QPushButton:hover {
    background-color: #1177bb;
    border-color: #1177bb;
}

QPushButton:pressed {
    background-color: #0a5a94;
    border-color: #0a5a94;
}

QPushButton:disabled {
    background-color: #3c3c3c;
    border-color: #464647;
    color: #6a6a6a;
}

QPushButton[objectName="primary_button"] {
    background-color: #0e639c;
    color: #ffffff;
    border: 1px solid #0e639c;
}

QPushButton[objectName="primary_button"]:hover {
    background-color: #1177bb;
    border-color: #1177bb;
}

QPushButton[objectName="primary_button"]:pressed {
    background-color: #0a5a94;
    border-color: #0a5a94;
}

QPushButton[objectName="secondary_button"] {
    background-color: transparent;
    color: #cccccc;
    border: 1px solid #464647;
}

QPushButton[objectName="secondary_button"]:hover {
    background-color: #2a2d2e;
    border-color: #007acc;
}

QPushButton[objectName="secondary_button"]:pressed {
    background-color: #094771;
    border-color: #007acc;
}

/* Breadcrumb buttons */
QPushButton[breadcrumb="true"] {
    background-color: transparent;
    border: none;
    color: #007acc;
    padding: 4px 6px;
    font-size: 12px;
}

QPushButton[breadcrumb="true"]:hover {
    background-color: #2a2d2e;
    border-radius: 2px;
}

/* === INPUT FIELDS === */
QLineEdit {
    background-color: #3c3c3c;
    border: 1px solid #464647;
    color: #cccccc;
    padding: 6px 8px;
    border-radius: 2px;
    font-size: 13px;
    min-height: 20px;
    selection-background-color: #094771;
}

QLineEdit:focus {
    border: 1px solid #007acc;
    background-color: #3c3c3c;
    outline: none;
}

QLineEdit:hover {
    border: 1px solid #6a6a6a;
}

QLineEdit:disabled {
    background-color: #2d2d30;
    border: 1px solid #464647;
    color: #6a6a6a;
}

QLineEdit[objectName="search_input"] {
    background-color: #3c3c3c;
    border: 1px solid #464647;
    color: #cccccc;
    padding: 6px 8px;
    border-radius: 2px;
    selection-background-color: #094771;
}

QLineEdit[objectName="search_input"]:focus {
    border: 1px solid #007acc;
    background-color: #3c3c3c;
    outline: none;
}

QLineEdit[objectName="search_input"]:hover {
    border: 1px solid #6a6a6a;
}

/* === COMBO BOXES === */
QComboBox {
    background-color: #3c3c3c;
    border: 1px solid #464647;
    color: #cccccc;
    padding: 6px 8px;
    border-radius: 2px;
    font-size: 13px;
    min-height: 20px;
}

QComboBox:focus {
    border: 1px solid #007acc;
    outline: none;
}

QComboBox:hover {
    border-color: #6a6a6a;
}

QComboBox::drop-down {
    border: none;
    background-color: transparent;
    width: 20px;
}

QComboBox::down-arrow {
    image: none;
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    border-top: 5px solid #cccccc;
    width: 0;
    height: 0;
}

QComboBox QAbstractItemView {
    background-color: #2d2d30;
    border: 1px solid #464647;
    selection-background-color: #2a2d2e;
    color: #cccccc;
    outline: none;
}

/* === LIST WIDGETS === */
QListWidget {
    background-color: #252526;
    color: #cccccc;
}

QListWidget::item {
    background-color: transparent;
    color: #cccccc;
}

QListWidget::item:hover {
    background-color: #2a2d2e;
}

QListWidget::item:selected {
    background-color: #094771;
    color: #cccccc;
}

QListWidget::item:selected:!active {
    background-color: #3a3d41;
}

/* Explorer file list */
QListWidget[objectName="explorer_file_list"] {
    background-color: #252526;
    color: #cccccc;
}

QListWidget[objectName="explorer_file_list"]::item {
    background-color: transparent;
    color: #cccccc;
}

QListWidget[objectName="explorer_file_list"]::item:hover {
    background-color: #2a2d2e;
}

QListWidget[objectName="explorer_file_list"]::item:selected {
    background-color: #094771;
    color: #cccccc;
}

QListWidget[objectName="explorer_file_list"]::item:selected:!active {
    background-color: #3a3d41;
}

/* === TREE WIDGETS === */
QTreeView {
    background-color: #252526;
    color: #cccccc;
}

QTreeView::item {
    background-color: transparent;
}

QTreeView::item:hover {
    background-color: #2a2d2e;
}

QTreeView::item:selected {
    background-color: #094771;
    color: #cccccc;
}

QTreeView::item:selected:!active {
    background-color: #3a3d41;
}

QTreeView::branch {
    background: transparent;
}

/* Search results table */
QTreeView[objectName="search_results_table"] {
    background-color: #252526;
    color: #cccccc;
    gridline-color: transparent;
    selection-background-color: #094771;
}

QTreeView[objectName="search_results_table"]::item {
    padding: 4px 8px;
    background-color: transparent;
    height: 20px;
}

QTreeView[objectName="search_results_table"]::item:hover {
    background-color: #2a2d2e;
}

QTreeView[objectName="search_results_table"]::item:selected {
    background-color: #094771;
    color: #cccccc;
}

QTreeView[objectName="search_results_table"]::item:selected:!active {
    background-color: #3a3d41;
}

QHeaderView[objectName="search_results_table"] {
    background-color: #252526;
    color: #969696;
    border-bottom: 1px solid #464647;
}

QHeaderView[objectName="search_results_table"]::section {
    background-color: #252526;
    color: #969696;
    border-bottom: 1px solid #464647;
}

QHeaderView[objectName="search_results_table"]::section:hover {
    background-color: #2a2d2e;
}

/* === TABLE WIDGETS === */
QTableWidget {
    background-color: #252526;
    color: #cccccc;
    gridline-color: transparent;
    selection-background-color: #094771;
}

QTableWidget::item {
    padding: 4px 8px;
    background-color: transparent;
    height: 20px;
}

QTableWidget::item:hover {
    background-color: #2a2d2e;
}

QTableWidget::item:selected {
    background-color: #094771;
    color: #cccccc;
}

QTableWidget::item:selected:!active {
    background-color: #3a3d41;
}

QHeaderView::section {
    background-color: #252526;
    color: #969696;
    border-bottom: 1px solid #464647;
}

QTableWidget QHeaderView::section:hover {
    background-color: #2a2d2e;
}

/* === TABS === */
QTabWidget::pane {
    background-color: #1e1e1e;
}

QTabBar::tab {
    background-color: #2d2d30;
    color: #969696;
    border-right: 1px solid #464647;
}

QTabBar::tab:selected {
    background-color: #1e1e1e;
    color: #ffffff;
    border-bottom: 2px solid #007acc;
}

QTabBar::tab:hover:!selected {
    background-color: #2a2d2e;
    color: #cccccc;
}

/* === GROUP BOXES === */
QGroupBox {
    background-color: transparent;
    border-color: #464647;
    color: #cccccc;
}

QGroupBox::title {
    color: #cccccc;
}

QGroupBox[objectName="group_box"] {
    border-color: #464647;
    color: #cccccc;
}

QGroupBox[objectName="group_box"]::title {
    color: #cccccc;
}

/* === SCROLL AREAS === */
QScrollArea {
    background-color: #1e1e1e;
}

QScrollBar:vertical {
    background-color: #252526;
}

QScrollBar::handle:vertical {
    background-color: #424242;
}

QScrollBar::handle:vertical:hover {
    background-color: #4f4f4f;
}

QScrollBar:horizontal {
    background-color: #252526;
}

QScrollBar::handle:horizontal {
    background-color: #424242;
}

QScrollBar::handle:horizontal:hover {
    background-color: #4f4f4f;
}

/* === LABELS === */
QLabel {
    color: #cccccc;
    background-color: transparent;
}

QLabel[objectName="info_label"] {
    color: #969696;
    background-color: transparent;
}

QLabel[objectName="description_label"] {
    color: #969696;
    background-color: transparent;
}

QLabel[objectName="version_label"] {
    color: #969696;
    background-color: transparent;
    font-weight: bold;
}

QLabel[objectName="status_label"] {
    color: #969696;
    background-color: transparent;
    font-size: 12px;
}

/* === CHECKBOXES === */
QCheckBox {
    color: #cccccc;
    background-color: transparent;
}

QCheckBox::indicator {
    background-color: #3c3c3c;
    border-color: #464647;
}

QCheckBox::indicator:checked {
    background-color: #007acc;
    border-color: #007acc;
}

QCheckBox::indicator:hover {
    border-color: #007acc;
}

/* === SPIN BOXES === */
QSpinBox {
    background-color: #3c3c3c;
    border: 1px solid #464647;
    color: #cccccc;
    padding: 4px 8px;
    border-radius: 2px;
    font-size: 13px;
    min-height: 20px;
}

QSpinBox:focus {
    border: 1px solid #007acc;
    background-color: #3c3c3c;
    outline: none;
}

QSpinBox:hover {
    border: 1px solid #6a6a6a;
}

QSpinBox:disabled {
    background-color: #2d2d30;
    border: 1px solid #464647;
    color: #6a6a6a;
}

QSpinBox::up-button {
    background-color: #464647;
    border: none;
    width: 16px;
}

QSpinBox::up-button:hover {
    background-color: #007acc;
}

QSpinBox::down-button {
    background-color: #464647;
    border: none;
    width: 16px;
}

QSpinBox::down-button:hover {
    background-color: #007acc;
}

/* === SLIDERS === */
QSlider::groove:horizontal {
    background-color: #2d2d30;
    height: 6px;
    border-radius: 3px;
}

QSlider::handle:horizontal {
    background-color: #007acc;
    width: 16px;
    height: 16px;
    border-radius: 8px;
    margin: -5px 0;
}

QSlider::handle:horizontal:hover {
    background-color: #1177bb;
}

/* === MENU BAR (TITLE BAR) === */
QMenuBar {
    background-color: #3c3c3c;
    color: #cccccc;
    border-bottom: 1px solid #464647;
    padding: 4px 0;
    font-size: 13px;
    height: 30px;
}

QMenuBar::item {
    background-color: transparent;
    padding: 6px 12px;
    margin: 0 1px;
    border-radius: 0;
}

QMenuBar::item:selected {
    background-color: #2a2d2e;
    color: #ffffff;
}

QMenuBar::item:pressed {
    background-color: #007acc;
    color: #ffffff;
}

/* === CONTEXT MENUS === */
QMenu {
    background-color: #2d2d30;
    border: 1px solid #464647;
    color: #cccccc;
}

QMenu::item:selected {
    background-color: #2a2d2e;
}

/* === STATUS BAR === */
QStatusBar {
    background-color: #007ACC;
    color: #ffffff;
    border: none;
    font-size: 12px;
    font-weight: normal;
    padding: 0;
    margin: 0;
}

QStatusBar::item {
    border: none;
    background-color: transparent;
    padding: 4px 12px;
    margin: 0;
}

QStatusBar QLabel {
    background-color: transparent;
    color: #ffffff;
    font-size: 12px;
    font-weight: normal;
    padding: 4px 0;
}

/* === TOOLTIPS === */
QToolTip {
    background-color: #2d2d30;
    color: #cccccc;
    border-color: #464647;
}

/* === BREADCRUMB NAVIGATION === */
QScrollArea#breadcrumb_scroll {
    background-color: #2d2d30;
    border: 1px solid #464647;
    border-radius: 0;
}

/* === EXTENSION ITEMS === */
QWidget#extension_item {
    background-color: transparent;
    color: #cccccc;
}

/* === ADDITIONAL WHITE EDGE FIXES === */

/* COMPREHENSIVE white edge elimination */

/* Force ALL input widgets to have dark borders - MAXIMUM PRIORITY */
* {
    border-color: #464647 !important;
}

*:focus {
    border-color: #007acc !important;
}

/* Force all input widgets to have dark borders */
QLineEdit, QTextEdit, QPlainTextEdit, QSpinBox, QDoubleSpinBox, QComboBox {
    border: 1px solid #464647 !important;
    background-color: #3c3c3c !important;
    color: #cccccc !important;
}

QLineEdit:focus, QTextEdit:focus, QPlainTextEdit:focus, QSpinBox:focus, QDoubleSpinBox:focus, QComboBox:focus {
    border: 1px solid #007acc !important;
    background-color: #3c3c3c !important;
}

QLineEdit:hover, QTextEdit:hover, QPlainTextEdit:hover, QSpinBox:hover, QDoubleSpinBox:hover, QComboBox:hover {
    border: 1px solid #6a6a6a !important;
}

/* Target any remaining input fields by type */
QAbstractSpinBox {
    border: 1px solid #464647 !important;
    background-color: #3c3c3c !important;
    color: #cccccc !important;
}

QAbstractSpinBox:focus {
    border: 1px solid #007acc !important;
}

/* Universal input field override */
QWidget[class*="Edit"], QWidget[class*="Input"], QWidget[class*="Field"] {
    border: 1px solid #464647 !important;
    background-color: #3c3c3c !important;
    color: #cccccc !important;
}

QWidget[class*="Edit"]:focus, QWidget[class*="Input"]:focus, QWidget[class*="Field"]:focus {
    border: 1px solid #007acc !important;
}

/* AGGRESSIVE white edge elimination with !important */

/* Force all widgets to have dark background */
QWidget {
    background-color: #1e1e1e !important;
    color: #cccccc !important;
}

/* Specific overrides for styled components */
QWidget[objectName="explorer_panel"] {
    background-color: #252526 !important;
}

QWidget[objectName="search_panel"] {
    background-color: #252526 !important;
}

QWidget[objectName="preferences_panel"] {
    background-color: #252526 !important;
}

QWidget[objectName="extensions_panel"] {
    background-color: #252526 !important;
}

QWidget[objectName="account_panel"] {
    background-color: #252526 !important;
}

QWidget[objectName="activity_bar"] {
    background-color: #2d2d30 !important;
}

/* Force dark backgrounds on all container types */
QMainWindow {
    background-color: #1e1e1e !important;
}

QFrame {
    background-color: #1e1e1e !important;
    border: 1px solid #464647;
}

QScrollArea {
    background-color: #1e1e1e !important;
    border: none;
}

QAbstractScrollArea {
    background-color: #1e1e1e !important;
}

QAbstractScrollArea::viewport {
    background-color: #1e1e1e !important;
}

QTabWidget {
    background-color: #1e1e1e !important;
}

QStackedWidget {
    background-color: #1e1e1e !important;
}

QDockWidget {
    background-color: #252526 !important;
}

QDockWidget > QWidget {
    background-color: #252526 !important;
}

/* Force list and tree backgrounds */
QListWidget {
    background-color: #252526 !important;
}

QTreeView {
    background-color: #252526 !important;
}

QTableWidget {
    background-color: #252526 !important;
}

/* Splitter handles - common source of white lines */
QSplitter::handle {
    background-color: #464647;
    border: none;
}

QSplitter::handle:horizontal {
    width: 1px;
    background-color: #464647;
}

QSplitter::handle:vertical {
    height: 1px;
    background-color: #464647;
}

QSplitter::handle:hover {
    background-color: #007acc;
}

/* Frame widgets */
QFrame {
    background-color: #1e1e1e;
    border: 1px solid #464647;
    color: #cccccc;
}

QFrame[frameShape="0"] {
    border: none;
    background-color: #1e1e1e;
}

QFrame[frameShape="1"] {
    border: 1px solid #464647;
    background-color: #1e1e1e;
}

QFrame[frameShape="2"] {
    border: 1px solid #464647;
    background-color: #1e1e1e;
}

QFrame[frameShape="3"] {
    border: 1px solid #464647;
    background-color: #1e1e1e;
}

QFrame[frameShape="4"] {
    border: 1px solid #464647;
    background-color: #1e1e1e;
}

QFrame[frameShape="5"] {
    border: 1px solid #464647;
    background-color: #1e1e1e;
}

/* Dialog boxes */
QDialog {
    background-color: #1e1e1e;
    color: #cccccc;
    border: 1px solid #464647;
}

/* Message boxes */
QMessageBox {
    background-color: #1e1e1e;
    color: #cccccc;
}

QMessageBox QLabel {
    background-color: transparent;
    color: #cccccc;
}

QMessageBox QPushButton {
    background-color: #3c3c3c;
    border: 1px solid #464647;
    color: #cccccc;
    padding: 6px 12px;
    min-width: 60px;
}

/* Progress bars */
QProgressBar {
    background-color: #3c3c3c;
    border: 1px solid #464647;
    color: #cccccc;
    text-align: center;
    border-radius: 2px;
}

QProgressBar::chunk {
    background-color: #007acc;
    border-radius: 2px;
}

/* Text edit widgets */
QTextEdit {
    background-color: #1e1e1e;
    border: 1px solid #464647;
    color: #cccccc;
    padding: 4px;
    border-radius: 2px;
    font-size: 13px;
}

QTextEdit:focus {
    border: 1px solid #007acc;
    outline: none;
}

QTextEdit:hover {
    border: 1px solid #6a6a6a;
}

QTextEdit::selection {
    background-color: #094771;
}

QPlainTextEdit {
    background-color: #1e1e1e;
    border: 1px solid #464647;
    color: #cccccc;
    padding: 4px;
    border-radius: 2px;
    font-size: 13px;
}

QPlainTextEdit:focus {
    border: 1px solid #007acc;
    outline: none;
}

QPlainTextEdit:hover {
    border: 1px solid #6a6a6a;
}

QPlainTextEdit::selection {
    background-color: #094771;
}

/* Dock widgets */
QDockWidget {
    background-color: #252526;
    color: #cccccc;
    border: 1px solid #464647;
}

QDockWidget::title {
    background-color: #2d2d30;
    color: #cccccc;
    padding: 4px;
    border: none;
}

QDockWidget::close-button, QDockWidget::float-button {
    background-color: transparent;
    border: none;
    color: #cccccc;
}

QDockWidget::close-button:hover, QDockWidget::float-button:hover {
    background-color: #2a2d2e;
}

/* Tool bars */
QToolBar {
    background-color: #2d2d30;
    border: 1px solid #464647;
    color: #cccccc;
}

QToolBar::separator {
    background-color: #464647;
    width: 1px;
    margin: 2px;
}

QToolButton {
    background-color: transparent;
    border: none;
    color: #cccccc;
    padding: 4px;
}

QToolButton:hover {
    background-color: #2a2d2e;
    border: 1px solid #007acc;
}

QToolButton:pressed {
    background-color: #094771;
}

/* Scroll bar improvements */
QScrollBar:vertical {
    background-color: #1e1e1e;
    width: 14px;
    border: none;
    margin: 0;
}

QScrollBar::handle:vertical {
    background-color: #424242;
    border-radius: 7px;
    min-height: 20px;
    margin: 2px;
}

QScrollBar::handle:vertical:hover {
    background-color: #4f4f4f;
}

QScrollBar::handle:vertical:pressed {
    background-color: #007acc;
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    background: none;
    border: none;
    height: 0px;
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    background: none;
}

QScrollBar:horizontal {
    background-color: #1e1e1e;
    height: 14px;
    border: none;
    margin: 0;
}

QScrollBar::handle:horizontal {
    background-color: #424242;
    border-radius: 7px;
    min-width: 20px;
    margin: 2px;
}

QScrollBar::handle:horizontal:hover {
    background-color: #4f4f4f;
}

QScrollBar::handle:horizontal:pressed {
    background-color: #007acc;
}

QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
    background: none;
    border: none;
    width: 0px;
}

QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    background: none;
}

/* Corner widget between scroll bars */
QScrollBar::corner {
    background-color: #1e1e1e;
    border: none;
}

/* Size grip */
QSizeGrip {
    background-color: transparent;
    width: 16px;
    height: 16px;
}

/* Radio buttons */
QRadioButton {
    background-color: transparent;
    color: #cccccc;
}

QRadioButton::indicator {
    width: 13px;
    height: 13px;
    border-radius: 7px;
    background-color: #3c3c3c;
    border: 1px solid #464647;
}

QRadioButton::indicator:checked {
    background-color: #007acc;
    border-color: #007acc;
}

QRadioButton::indicator:hover {
    border-color: #007acc;
}

/* Calendar widget */
QCalendarWidget {
    background-color: #1e1e1e;
    color: #cccccc;
    border: 1px solid #464647;
}

QCalendarWidget QTableView {
    background-color: #1e1e1e;
    color: #cccccc;
}

QCalendarWidget QTableView::item:selected {
    background-color: #094771;
}

/* Font dialog */
QFontDialog {
    background-color: #1e1e1e;
    color: #cccccc;
}

/* Color dialog */
QColorDialog {
    background-color: #1e1e1e;
    color: #cccccc;
}

/* File dialog */
QFileDialog {
    background-color: #1e1e1e;
    color: #cccccc;
}

QFileDialog QListView {
    background-color: #252526;
    color: #cccccc;
}

QFileDialog QTreeView {
    background-color: #252526;
    color: #cccccc;
}

/* Ensure all container widgets have proper dark background */
QWidget[class*="Container"],
QWidget[class*="Panel"],
QWidget[class*="View"],
QWidget[class*="Layout"] {
    background-color: #1e1e1e;
    color: #cccccc;
}

/* Additional fixes for stubborn white edges */

/* Dock widget content areas */
QDockWidget > QWidget {
    background-color: #252526;
}

/* Panel content areas */
QWidget[objectName*="_panel"] {
    background-color: #252526;
}

QWidget[objectName*="_panel"] > QWidget {
    background-color: #252526;
}

/* Activity bar and sidebar specific fixes */
QWidget[objectName="activity_bar"] {
    background-color: #2d2d30;
}

QWidget[objectName="activity_bar"] > QWidget {
    background-color: #2d2d30;
}

/* Sidebar dock widget fixes */
QWidget[objectName*="sidebar"] {
    background-color: #252526;
}

QWidget[objectName*="sidebar"] > QWidget {
    background-color: #252526;
}

/* Explorer and file list specific fixes */
QWidget[objectName*="explorer"] {
    background-color: #252526;
}

QWidget[objectName*="explorer"] > QWidget {
    background-color: #252526;
}

/* Search panel specific fixes */
QWidget[objectName*="search"] {
    background-color: #252526;
}

QWidget[objectName*="search"] > QWidget {
    background-color: #252526;
}

/* Tab content area fixes */
QTabWidget::pane {
    background-color: #1e1e1e;
    border: 1px solid #464647;
    border-top: none;
}

/* Widget focus and selection fixes */
QWidget:focus {
    background-color: #1e1e1e;
    outline: none;
}

/* Generic container widget fix */
QWidget[accessibleName=""] {
    background-color: #1e1e1e;
}

/* Breadcrumb and navigation specific fixes */
QWidget[objectName*="breadcrumb"] {
    background-color: #2d2d30;
}

QWidget[objectName*="breadcrumb"] > QWidget {
    background-color: #2d2d30;
}

/* Status and info widgets */
QWidget[objectName*="status"] {
    background-color: #1e1e1e;
}

QWidget[objectName*="info"] {
    background-color: #1e1e1e;
}

/* === FINAL OVERRIDE - ELIMINATE ALL WHITE BORDERS === */
/* This section uses the most aggressive approach to eliminate white borders */

/* Override any widget that might have a white or light border */
QWidget[style*="border"], QWidget[border*="white"], QWidget[border*="#fff"] {
    border: 1px solid #464647 !important;
}

/* Target input widgets by their likely object names */
QWidget[objectName*="input"], QWidget[objectName*="edit"], QWidget[objectName*="field"], 
QWidget[objectName*="text"], QWidget[objectName*="entry"], QWidget[objectName*="box"] {
    border: 1px solid #464647 !important;
    background-color: #3c3c3c !important;
    color: #cccccc !important;
}

QWidget[objectName*="input"]:focus, QWidget[objectName*="edit"]:focus, QWidget[objectName*="field"]:focus,
QWidget[objectName*="text"]:focus, QWidget[objectName*="entry"]:focus, QWidget[objectName*="box"]:focus {
    border: 1px solid #007acc !important;
}

/* Nuclear option - override ANY widget that has a border */
QWidget[style*="border-color: rgb(255, 255, 255)"],
QWidget[style*="border-color: white"],
QWidget[style*="border-color: #ffffff"],
QWidget[style*="border-color: #fff"] {
    border-color: #464647 !important;
}

/* Override Qt's default input field styling */
QLineEdit[style*="border"], QTextEdit[style*="border"], QPlainTextEdit[style*="border"],
QSpinBox[style*="border"], QDoubleSpinBox[style*="border"], QComboBox[style*="border"] {
    border: 1px solid #464647 !important;
    background-color: #3c3c3c !important;
}

/* Force dark styling on ALL frame-based widgets */
QFrame[frameStyle*="1"], QFrame[frameStyle*="2"], QFrame[frameStyle*="3"],
QFrame[frameStyle*="4"], QFrame[frameStyle*="5"], QFrame[frameStyle*="6"] {
    border: 1px solid #464647 !important;
    background-color: #1e1e1e !important;
}

/* === FINAL DESPERATE ATTEMPT - TARGET ALL TEXT INPUT FIELDS === */
/* If all else fails, this should catch any remaining white-bordered inputs */

/* Every possible QLineEdit variant with maximum specificity */
QWidget QLineEdit, QFrame QLineEdit, QDialog QLineEdit, QMainWindow QLineEdit,
QDockWidget QLineEdit, QScrollArea QLineEdit, QGroupBox QLineEdit, QTabWidget QLineEdit {
    border: 1px solid #464647 !important;
    background-color: #3c3c3c !important;
    color: #cccccc !important;
    padding: 6px 8px !important;
    border-radius: 2px !important;
    font-size: 13px !important;
}

QWidget QLineEdit:focus, QFrame QLineEdit:focus, QDialog QLineEdit:focus, QMainWindow QLineEdit:focus,
QDockWidget QLineEdit:focus, QScrollArea QLineEdit:focus, QGroupBox QLineEdit:focus, QTabWidget QLineEdit:focus {
    border: 1px solid #007acc !important;
    background-color: #3c3c3c !important;
}

/* Catch filter inputs specifically */
QLineEdit[placeholderText*="filter"], QLineEdit[placeholderText*="Filter"], 
QLineEdit[placeholderText*="search"], QLineEdit[placeholderText*="Search"] {
    border: 1px solid #464647 !important;
    background-color: #3c3c3c !important;
    color: #cccccc !important;
}

QLineEdit[placeholderText*="filter"]:focus, QLineEdit[placeholderText*="Filter"]:focus,
QLineEdit[placeholderText*="search"]:focus, QLineEdit[placeholderText*="Search"]:focus {
    border: 1px solid #007acc !important;
}

/* === EMERGENCY OVERRIDE - ALL POSSIBLE INPUT WIDGETS === */
/* This is the nuclear option to force ALL input-like widgets to have dark borders */

/* Target every possible input widget type */
QLineEdit, QTextEdit, QPlainTextEdit, QSpinBox, QDoubleSpinBox, 
QComboBox, QAbstractSpinBox, QAbstractItemView, QAbstractSlider {
    border: 1px solid #464647 !important;
    background-color: #3c3c3c !important;
    color: #cccccc !important;
}

/* Focus states for all input widgets */
QLineEdit:focus, QTextEdit:focus, QPlainTextEdit:focus, QSpinBox:focus, 
QDoubleSpinBox:focus, QComboBox:focus, QAbstractSpinBox:focus {
    border: 1px solid #007acc !important;
    background-color: #3c3c3c !important;
}

/* Override any widget that inherits from QWidget and has editable content */
QWidget[accessibleRole="20"], QWidget[accessibleRole="21"], QWidget[accessibleRole="22"] {
    border: 1px solid #464647 !important;
    background-color: #3c3c3c !important;
    color: #cccccc !important;
}

/* Target widgets by their likely class names */
QWidget[class*="LineEdit"], QWidget[class*="TextEdit"], QWidget[class*="Input"], 
QWidget[class*="Editor"], QWidget[class*="Field"], QWidget[class*="Box"] {
    border: 1px solid #464647 !important;
    background-color: #3c3c3c !important;
    color: #cccccc !important;
}

/* Target any widget that might be used as an input field */
QWidget[style*="QLineEdit"], QWidget[style*="QTextEdit"], QWidget[style*="input"] {
    border: 1px solid #464647 !important;
    background-color: #3c3c3c !important;
    color: #cccccc !important;
}

/* Force override on any QFrame that might be used as input container */
QFrame[style*="border"], QFrame[objectName*="input"], QFrame[objectName*="edit"],
QFrame[objectName*="filter"], QFrame[objectName*="search"], QFrame[objectName*="field"] {
    border: 1px solid #464647 !important;
    background-color: #3c3c3c !important;
}

/* Absolutely force all QLineEdit variants */
QLineEdit, QLineEdit[style*="border"], QLineEdit[objectName*="filter"], 
QLineEdit[objectName*="search"], QLineEdit[objectName*="input"] {
    border: 1px solid #464647 !important;
    background-color: #3c3c3c !important;
    color: #cccccc !important;
    padding: 6px 8px !important;
    border-radius: 2px !important;
}

QLineEdit:focus, QLineEdit[style*="border"]:focus, QLineEdit[objectName*="filter"]:focus,
QLineEdit[objectName*="search"]:focus, QLineEdit[objectName*="input"]:focus {
    border: 1px solid #007acc !important;
    background-color: #3c3c3c !important;
}

/* === EXPLORER COMPONENTS (Dark Theme) === */
/* Status labels */
QLabel[class="status-label"] {
    color: #969696;
}

/* Breadcrumb navigation area */
QScrollArea[class="breadcrumb-scroll"] {
    border: 1px solid #464647;
    background-color: #1e1e1e;
}

/* Breadcrumb buttons (navigation links) */
QPushButton[class="breadcrumb-button"] {
    color: #4fc3f7;
}

QPushButton[class="breadcrumb-button"]:hover {
    background-color: #2a2d2e;
}

QPushButton[class="breadcrumb-button"]:pressed {
    background-color: #383b3d;
}

/* Breadcrumb separators */
QLabel[class="breadcrumb-separator"] {
    color: #969696;
}

/* === INFO AND HELP LABELS (Dark Theme) === */
QLabel[class="info-label"] {
    color: #969696;
}
